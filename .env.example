# LaunchDarkly Configuration (required)
LD_SDK_KEY=sdk-xxxxx         # Your LaunchDarkly SDK key
LD_API_KEY=api-xxxxx         # Your LaunchDarkly API key
LAUNCHDARKLY_PROJECT_KEY=orchestrator-agents

# Model Provider API Keys (at least one required)
ANTHROPIC_API_KEY=sk-ant-xxxxx    # For Claude models via Anthropic
OPENAI_API_KEY=sk-xxxxx           # For GPT models via OpenAI

# AWS Configuration (optional - for Bedrock models)
AWS_PROFILE=your-profile
AWS_REGION=us-east-1

# Optional Debug Settings
DEBUG=false
LANGGRAPH_RECURSION_LIMIT=30